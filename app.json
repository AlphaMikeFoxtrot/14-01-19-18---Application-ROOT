{
  "expo": {
    "name": "NASR Admin",
    "description": "This project is really great.",
    "slug": "nasr-v2-admin",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/logo/logo_big.png",
    "splash": {
      "image": "./app/assets/logo/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {
      "backgroundColor": "#1b5e20",
      "barStyle": "light-content"
    },
    "android": {
      "package": "com.smemetrics.nasr"
    },
    "notification": {
      "icon": "./app/assets/logo/final_notification_icon.png", 
      "color": "#000000"
    }
  }
}
